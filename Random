-- Scripts:

local function FJRMRZ_fake_script() -- TextButton.LocalScript 
	local UIS = game:GetService('UserInputService')
	
	local frame = TextButton
	
	
	
	local dragToggle = nil
	
	local dragSpeed = 0
	
	local dragStart = nil
	
	local startPos = nil
	
	
	
	local function updateInput(input)
	
		local delta = input.Position - dragStart
	
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
	
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	
	end
	
	
	
	frame.InputBegan:Connect(function(input)
	
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
	
			dragToggle = true
	
			dragStart = input.Position
	
			startPos = frame.Position
	
			input.Changed:Connect(function()
	
				if input.UserInputState == Enum.UserInputState.End then
	
					dragToggle = false
	
				end
	
			end)
	
		end
	
	end)
	
	
	
	UIS.InputChanged:Connect(function(input)
	
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	
			if dragToggle then
	
				updateInput(input)
	
			end
	
		end
	
	end)
end
FJRMRZ_fake_script()
local function HWFUJ_fake_script() -- ScreenGui.LocalScript 
	local modmenu = TextButton
	local mainframe = modmenu.Frame
	modmenu.MouseButton2Up:Connect(function()
		if mainframe.Visible == false then
			modmenu.Size = UDim2.new(0,335,0,10)
			mainframe.Visible = true
			wait()
		else
			modmenu.Size = UDim2.new(0,10,0,10)
			mainframe.Visible = false
			wait()
		end
	end)
end
HWFUJ_fake_script()

--CreateRandomShitOnGui:

local gap = UDim2.new(0.1,0,0.1,0)
local FramePosition = UDim2.new(0,0,1,0)
local function CreateDropDownMenu(WhichFrame,Name,Text,targetlist,IsAPreSetTable,position) -- Frame_2.LocalScript
	local TextButton_2 = Instance.new("TextButton")
	local ScrollingFrame = Instance.new("ScrollingFrame")
	local UIListLayout = Instance.new("UIListLayout")
	local TextLabel = Instance.new("TextLabel")
	local gap = UDim2.new(0,0,0,0)
	TextButton_2.Parent = WhichFrame
	TextButton_2.Name = Name
	TextButton_2.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	TextButton_2.BackgroundTransparency = 0.500
	if position == 1 then
		gap = UDim2.new(0.1,0,0.1,0)
	elseif position == 2 then
		gap = UDim2.new(0.1,0,0.5,0)
	elseif position == 3 then
		gap = UDim2.new(0.6,0,0.1,0)
	elseif position == 4 then
		gap = UDim2.new(0.6,0,0.5,0) 
	end
	TextButton_2.Position = gap
	TextButton_2.Size = UDim2.new(0, 100, 0, 20)
	TextButton_2.Font = Enum.Font.GothamBold
	TextButton_2.Text = ""
	TextButton_2.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextButton_2.TextSize = 14.000
	TextLabel.Parent = TextButton_2
	TextLabel.Name = Text
	TextLabel.Position = UDim2.new(0,0,1,0)
	TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.BorderColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.BorderSizePixel = 0
	TextLabel.Size = UDim2.new(0, 100, 0, 20)
	TextLabel.Text = Text
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 14.000
	TextLabel.Font = Enum.Font.SourceSans
	ScrollingFrame.Parent = TextButton_2
	ScrollingFrame.Active = true
	ScrollingFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	ScrollingFrame.BackgroundTransparency = 0.700
	ScrollingFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
	ScrollingFrame.Position = UDim2.new(0, 0, 1, 0)
	ScrollingFrame.Size = UDim2.new(0, 100, 0, 70)
	ScrollingFrame.Visible = false
	ScrollingFrame.ScrollBarThickness = 5
	UIListLayout.Parent = ScrollingFrame
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	local menu = ScrollingFrame
	local mainbutton = TextButton_2
	local HowManyButtons = UDim2.new(0,0,0,0)
	for _,v in pairs(targetlist) do
		if IsAPreSetTable and not menu:FindFirstChild(v.Name) then
			local Button = Instance.new("TextButton")
			Button.Parent = menu
			Button.BackgroundColor3 = Color3.new(0,0,0)
			Button.TextColor3 = Color3.new(255, 255, 255)
			Button.Size = UDim2.new(0,100,0,20)
			Button.Font = "GothamBold"
			Button.BackgroundTransparency = 0.7
			Button.Text = v
			Button.Name = v
			wait()
			HowManyButtons = HowManyButtons + UDim2.new(0,0,0,20)
		elseif v:IsA("Model") and not menu:FindFirstChild(v.Name) then
			local Button = Instance.new("TextButton")
			Button.Parent = menu
			Button.BackgroundColor3 = Color3.new(0,0,0)
			Button.TextColor3 = Color3.new(255, 255, 255)
			Button.Size = UDim2.new(0,100,0,20)
			Button.Font = "GothamBold"
			Button.BackgroundTransparency = 0.7
			Button.Text = v.Name
			Button.Name = v.Name
			wait()
			HowManyButtons = HowManyButtons + UDim2.new(0,0,0,20)
		end
	end
	ScrollingFrame.CanvasSize = HowManyButtons
	for _,v in pairs(menu:GetChildren()) do
		if v.ClassName == ("TextButton") then
			v.MouseButton1Click:Connect(function()
				mainbutton.Text = v.Text
				mainbutton.ScrollingFrame.Visible = false
			end)
		end
	end
	mainbutton.MouseButton1Click:Connect(function()
		if mainbutton.ScrollingFrame.Visible == false then
			mainbutton.ScrollingFrame.Visible = true
		else
			mainbutton.ScrollingFrame.Visible = false
		end	
	end)
end
local function CreateCheckBox(WhichFrame,Name,position)
	local TextLabel = Instance.new("TextLabel")
	local ImageButton = Instance.new("ImageButton")
	TextLabel.Parent = WhichFrame
	TextLabel.Name = Name
	TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.BorderColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.BorderSizePixel = 0
	if position == 1 then
		gap = UDim2.new(0.1,0,0.1,0)
	elseif position == 2 then
		gap = UDim2.new(0.1,0,0.5,0)
	elseif position == 3 then
		gap = UDim2.new(0.6,0,0.1,0)
	elseif position == 4 then
		gap = UDim2.new(0.6,0,0.5,0)
	elseif position == 5 then
		gap = UDim2.new(0.349,0,0.81,0)
	end
	TextLabel.Position = gap
	TextLabel.Size = UDim2.new(0, 100, 0, 20)
	TextLabel.Font = Enum.Font.GothamBold
	TextLabel.Text = Name
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 14.000
	ImageButton.Parent = TextLabel
	ImageButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	ImageButton.BackgroundTransparency = 0.700
	ImageButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	ImageButton.BorderSizePixel = 2
	ImageButton.Position = UDim2.new(1.05, 0, 0, 0)
	ImageButton.Size = UDim2.new(0, 20, 0, 20)
	ImageButton.HoverImage = "http://www.roblox.com/asset/?id=6059207872"
	local button = ImageButton
	button.MouseButton1Click:Connect(function()
		if button.Image == "" then
			button.Image = "http://www.roblox.com/asset/?id=6059207872"
			wait(1)
		else
			button.Image = ""
			wait(1)
		end	
	end)
end
local function CreateTextBox(WhichFrame,Name,Text,position)
	local TextBox = Instance.new("TextBox")
	local TextLabel = Instance.new("TextLabel")
	TextBox.Parent = WhichFrame
	TextBox.Name = Name
	TextBox.Text = ""
	TextBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	TextBox.BackgroundTransparency = 0.5
	TextBox.BorderColor3 = Color3.fromRGB(255, 255, 255)
	TextBox.BorderSizePixel = 0
	if position == 1 then
		gap = UDim2.new(0.1,0,0.1,0)
	elseif position == 2 then
		gap = UDim2.new(0.1,0,0.5,0)
	elseif position == 3 then
		gap = UDim2.new(0.6,0,0.1,0)
	elseif position == 4 then
		gap = UDim2.new(0.6,0,0.5,0)
	elseif position == 5 then
		gap = UDim2.new(0.349,0,0.81,0)
	end
	TextBox.Position = gap
	TextBox.Size = UDim2.new(0, 100, 0, 20)
	TextBox.Font = Enum.Font.GothamBold
	TextBox.Text = Name
	TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextBox.TextSize = 14.000
	TextLabel.Parent = TextBox
	TextLabel.Name = Text
	TextLabel.Position = UDim2.new(0,0,1,0)
	TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.BorderColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.BorderSizePixel = 0
	TextLabel.Size = UDim2.new(0, 100, 0, 20)
	TextLabel.Text = Text
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 14.000
	TextLabel.Font = Enum.Font.SourceSans
end
local function CreateNewFrame(Name,Text,visible)
	local NewTab = Instance.new("TextButton")
	local Frame1 = Instance.new("Frame")
	NewTab.Parent = Frame
	NewTab.Name = Name
	NewTab.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	NewTab.Size = UDim2.new(0, 167, 0, 20)
	NewTab.Font = Enum.Font.SourceSans
	NewTab.Text = Text
	NewTab.TextSize = 14.000

	Frame1.Parent = NewTab
	Frame1.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Frame1.BackgroundTransparency = 0.500
	Frame1.Position = FramePosition
	FramePosition = FramePosition + UDim2.new(-1,0,0,0)
	Frame1.Size = UDim2.new(0, 335, 0, 239)
	Frame1.Visible = false
	if visible then
		NewTab.TextColor3 = Color3.fromRGB(0, 255, 255)
		NewTab.BackgroundTransparency = 0.500
		Frame1.Visible = true
	else
		NewTab.TextColor3 = Color3.fromRGB(255, 255, 255)
		NewTab.BackgroundTransparency = 0.600
		Frame1.Visible = false
	end
	
	NewTab.MouseButton1Up:Connect(function()
		NewTab.Frame.Visible = true
		NewTab.BackgroundTransparency = 0.5
		NewTab.TextColor3 = Color3.new(0, 85, 255)
		for _,v in pairs(Frame:GetChildren()) do
			if v:IsA("TextButton") and v.Name ~= NewTab.Name then
				v.Frame.Visible = false
				v.BackgroundTransparency = 0.6
				v.TextColor3 = Color3.new(255, 255, 255)
			end
		end
	end)
end

